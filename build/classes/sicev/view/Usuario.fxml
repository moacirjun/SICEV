<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" prefHeight="450.0" prefWidth="778.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sicev.controller.UsuarioController">
  <center>
    <VBox prefHeight="200.0" prefWidth="100.0">
      <GridPane fx:id="paneCampos" minHeight="-1.0" prefHeight="420.0" prefWidth="400.0">
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <Label text="Código" />
          <TextField fx:id="edtCodigo" disable="true" prefWidth="200.0" />
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </VBox>
        <VBox prefHeight="40.0" prefWidth="567.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="0">
          <Label text="Nome" />
          <TextField fx:id="edtNome" prefWidth="200.0" />
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <Label text="Login" />
          <TextField id="edtValor" fx:id="edtLogin" prefWidth="200.0" />
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </VBox>
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" maxWidth="188.0" minWidth="10.0" prefWidth="56.0" />
          <ColumnConstraints hgrow="ALWAYS" maxWidth="284.0" minWidth="10.0" prefWidth="153.0" />
          <ColumnConstraints hgrow="ALWAYS" maxWidth="288.0" minWidth="10.0" prefWidth="139.0" />
        </columnConstraints>
        <padding>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <rowConstraints>
          <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="47.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="47.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <VBox.margin>
          <Insets top="57.0" />
        </VBox.margin>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
          <Label text="Senha" />
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
          <PasswordField fx:id="edtSenha" prefWidth="200.0" />
        </VBox>
      </GridPane>
      <HBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
        <Button fx:id="btnSalvar" mnemonicParsing="false" onAction="#handleBtnSalvarAction" prefWidth="94.0" text="Salvar">
          <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../images/icons8_Save_30px.png" />
              </image>
            </ImageView>
          </graphic>
        </Button>
        <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#handleBtnCancelarAction" prefWidth="94.0" text="Cancelar">
          <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../images/icons8_Cancel_30px.png" />
              </image>
            </ImageView>
          </graphic>
        </Button>
        <padding>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
        <VBox.margin>
          <Insets bottom="49.0" />
        </VBox.margin>
      </HBox>
    </VBox>
  </center>
  <left>
    <VBox fx:id="tableContainer" prefHeight="460.0" prefWidth="-1.0">
      <VBox id="tableContainer">
        <HBox minHeight="-Infinity" minWidth="-1.0" prefHeight="57.0" prefWidth="-1.0">
          <VBox prefHeight="-1.0" prefWidth="312.0">
            <padding>
              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <Label text="Pesquisar" />
            <TextField fx:id="edtPesquisar" prefWidth="200.0" />
          </VBox>
          <VBox prefHeight="57.0">
            <padding>
              <Insets bottom="5.0" left="5.0" right="5.0" top="22.0" />
            </padding>
            <Button fx:id="btnRemoveFiltro" onAction="#handleBtnRemoveFiltro" prefHeight="22.0" text="Limpar" />
          </VBox>
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </HBox>
        <TableView id="tabelaProdutos" fx:id="tabelaUsuarios" prefHeight="-1.0" prefWidth="-1.0">
          <columns>
            <TableColumn prefWidth="75.0" text="Código">
              <cellValueFactory>
                <PropertyValueFactory property="id" />
              </cellValueFactory>
            </TableColumn>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="197.0" text="Nome">
              <cellValueFactory>
                <PropertyValueFactory property="nome" />
              </cellValueFactory>
            </TableColumn>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="94.0" text="Login">
              <cellValueFactory>
                <PropertyValueFactory property="login" />
              </cellValueFactory>
            </TableColumn>
          </columns>
          <VBox.margin>
            <Insets left="10.0" right="10.0" />
          </VBox.margin>
        </TableView>
      </VBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
        <ToggleButton fx:id="btnNovo" mnemonicParsing="false" onAction="#handleBtnNovoAction" prefWidth="94.0" text="Novo">
          <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../images/icons8_Plus_30px.png" />
              </image>
            </ImageView>
          </graphic>
        </ToggleButton>
        <ToggleButton fx:id="btnEditar" mnemonicParsing="false" onAction="#handleBtEditarAction" prefWidth="94.0" text="Editar">
          <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../images/icons8_Edit_30px.png" />
              </image>
            </ImageView>
          </graphic>
        </ToggleButton>
        <Button fx:id="btnExcluir" mnemonicParsing="false" onAction="#handleBtnExcluirAction" prefWidth="94.0" text="Excuir">
          <graphic>
            <ImageView fitHeight="23.9998779296875" fitWidth="23.9998779296875" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@../images/icons8_Delete_30px.png" />
              </image>
            </ImageView>
          </graphic>
        </Button>
        <padding>
          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
        </padding>
      </HBox>
    </VBox>
  </left>
  <stylesheets>
    <URL value="@usuario.css" />
  </stylesheets>
</BorderPane>
